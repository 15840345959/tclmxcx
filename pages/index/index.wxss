/**index.wxss**/

page {
  background-color: #f1f1f1;
}

.slide-image {
  height: 100%;
  width: 100%;
}

.model {
  background: #fff;
  border-radius: 10%;
}

.themes-list {
  background: #fff;
  display: block;
  margin-bottom: 20rpx;
}

.themes-list-box {
  display: block;
  position: relative;
  padding: 32rpx 32rpx;
}

.themes-list-thumbnail {
  position: absolute;
  left: 32rpx;
  top: 32rpx;
  height: 160rpx;
  width: 160rpx;
}

.themes-list-img {
  width: 160rpx;
  height: 160rpx;
  border-radius: 20rpx;
  border: 1px solid #f2f2f2;
}

.themes-list-main {
  margin-left: 160rpx;
}

.themes-list-name {
  font-size: 30rpx;
  color: #606898;
  height: 55rpx;
  line-height: 50rpx;
  overflow: hidden;
  text-align: right;
}

.themes-list-description {
  font-size: 26rpx;
  color: #999;
  height: 50rpx;
  line-height: 60rpx;
  overflow: hidden;
  text-align: right;
}

.no-data-image {
  width: 235rpx;
  height: 197rpx;
}

.animation-car {
  animation: car 1s;
  position:relative;
}

@keyframes car
{
  from {
    top: -50px;
  }
  to {
    top: 0px;
  }
}

.a {
  position: absolute
}
